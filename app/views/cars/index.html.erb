<h1>Listing cars</h1>

<table>
  <thead>
    <tr>
      <th>Cylinders</th>
      <th>Displ</th>
      <th>Drivetrain</th>
      <th>Fueltype</th>
      <th>Mpg</th>
      <th>Make</th>
      <th>Model</th>
      <th>Trany</th>
      <th>Class</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.cylinders %></td>
        <td><%= car.displ %></td>
        <td><%= car.driveTrain %></td>
        <td><%= car.fuelType %></td>
        <td><%= car.mpg %></td>
        <td><%= car.make %></td>
        <td><%= car.model %></td>
        <td><%= car.trany %></td>
        <td><%= car.class %></td>
        <td><%= car.year %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
